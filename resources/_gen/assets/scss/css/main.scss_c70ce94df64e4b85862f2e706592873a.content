  .post {
    font-size: 0.9em;
    line-height: 1.6em; }
  .post-template .post {
    padding-bottom: 1rem; }
  .post-template .post-header {
    padding: 40px 0; }
  h1 {
    font-size: 4.8rem;
    text-indent: -2px; }
  h2 {
    font-size: 3.8rem; }
  h3 {
    font-size: 3.3rem; }
  h4 {
    font-size: 2.8rem; } }

/* ==========================================================================
      10. Media Queries - Smaller than 500px
      ========================================================================== */
@media only screen and (max-width: 500px) {
  .fixed-nav {
    display: none; }
  .post-holder {
    padding-top: 20px; }
  #blog-logo img {
    max-height: 80px; }
  .inner,
  .pagination {
    width: auto;
    margin-left: 16px;
    margin-right: 16px; }
  .post {
    width: auto;
    margin-left: 16px;
