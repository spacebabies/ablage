  a.btn {
    margin-top: 10px; }
  .blog-title {
    font-size: 3rem; }
  .blog-description {
    font-size: 2.2rem; }
  h1,
  h2 {
    font-size: 3rem;
    line-height: 1.1em;
    letter-spacing: -1px; }
  h3 {
    font-size: 2.8rem; }
  h4 {
    font-size: 2.3rem; }
  .post-template .post {
    padding-bottom: 0; }
  .post-template .post-header {
    padding: 30px 0; }
  .post-meta {
    font-size: 1.3rem; }
  .post-footer {
    padding: 4rem 0;
    text-align: center; }
  .post-footer .author {
    margin: 0 0 2rem 0;
    padding: 0 0 1.6rem 0;
    border-bottom: #ebf2f6 1px dashed; }
  .post-footer .share {
    position: static;
    width: auto; }
  .post-footer .share a {
    margin: 1.4rem 0.8rem 0 0.8rem; }
  .older-posts,
  .newer-posts {
    position: static;
    margin: 10px 0; }
  .page-number {
    display: block; }
  .site-footer {
    font-size: 1.1rem; } }

